<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:tools="http://schemas.android.com/tools"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="vertical"
              tools:context="com.aiton.bamin.changtukepiao.Zeverything.everything_fragment.MineEverythingFragment">


    <RelativeLayout
        android:id="@+id/relativeLayout12"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_gravity="center_horizontal"
        android:background="@color/title_bar">

        <TextView
            android:id="@+id/textView16"
            style="@style/title_bar_text_style"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="我的"/>
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="160dp"
        android:background="@mipmap/a01">

        <RelativeLayout
            android:id="@+id/loginED"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true">

            <ImageView
                android:id="@+id/imageView17"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:src="@mipmap/a02"/>

            <RelativeLayout
                android:id="@+id/rela_login"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/imageView17"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="10dp"
                android:visibility="invisible">

                <TextView
                    android:id="@+id/textView18"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:layout_toEndOf="@+id/textView17"
                    android:layout_toRightOf="@+id/textView17"
                    android:text="普卡会员"
                    android:textColor="@color/white"
                    android:textSize="@dimen/normalTextSize"/>
            </RelativeLayout>

            <TextView
                android:id="@+id/textView_login"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/rela_login"
                android:layout_centerHorizontal="true"
                android:text="15871105320"
                android:textColor="@color/white"
                android:textSize="@dimen/normalTextSize"
                android:visibility="invisible"/>

            <TextView
                android:id="@+id/textView_unLogin"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/rela_login"
                android:layout_centerHorizontal="true"
                android:text="登录/注册"
                android:textColor="@color/white"
                android:textSize="15dp"/>
        </RelativeLayout>
    </RelativeLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/scrollView8">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:layout_gravity="center_horizontal"
                android:background="@color/white"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView20"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:background="@drawable/layout_selector_white_system_gray"
                    android:text="0分\n积分"
                    android:textSize="@dimen/normalTextSize"/>

                <TextView
                    android:id="@+id/textView21"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@drawable/layout_selector_white_system_gray"
                    android:gravity="center"
                    android:text="0张\n优惠卡"
                    android:textSize="@dimen/normalTextSize"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:background="@color/white"
                android:orientation="vertical">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:background="@drawable/layout_selector_white_system_gray"
                    android:layout_height="50dp">

                    <ImageView
                        android:id="@+id/imageView18"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="15dp"
                        android:layout_marginStart="21dp"
                        android:src="@mipmap/ic_me_coupon"/>

                    <TextView
                        android:id="@+id/textView22"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"
                        android:layout_marginStart="12dp"
                        android:layout_toEndOf="@+id/imageView18"
                        android:layout_toRightOf="@+id/imageView18"
                        android:text="积分商城"
                        android:textSize="@dimen/normalTextSize"/>

                    <ImageView
                        android:id="@+id/imageView19"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginEnd="20dp"
                        android:layout_marginRight="20dp"
                        android:src="@mipmap/ic_me_arrow_right_gray"/>
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/line_width"
                    android:background="@color/line_gray">
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:background="@drawable/layout_selector_white_system_gray"
                    android:layout_height="50dp">

                    <ImageView
                        android:id="@+id/iv_evething_day_recommend"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="15dp"
                        android:layout_marginStart="21dp"
                        android:src="@mipmap/ic_me_driver"/>

                    <TextView
                        android:id="@+id/tv_evething_day_recommend"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"
                        android:layout_marginStart="12dp"
                        android:layout_toEndOf="@+id/iv_evething_day_recommend"
                        android:layout_toRightOf="@+id/iv_evething_day_recommend"
                        android:text="每日推荐"
                        android:textSize="@dimen/normalTextSize"/>

                    <ImageView
                        android:id="@+id/imageView21"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginEnd="20dp"
                        android:layout_marginRight="20dp"
                        android:src="@mipmap/ic_me_arrow_right_gray"/>
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/line_width"
                    android:background="@color/line_gray">
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rl_mine_evething_soft_info"
                    android:layout_width="match_parent"
                    android:background="@drawable/layout_selector_white_system_gray"
                    android:layout_height="50dp">

                    <ImageView
                        android:id="@+id/iv_evething_soft_info"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="15dp"
                        android:layout_marginStart="21dp"
                        android:src="@mipmap/aboutsoft"/>

                    <TextView
                        android:id="@+id/tv_evething_soft_info"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"
                        android:layout_marginStart="12dp"
                        android:layout_toEndOf="@+id/iv_evething_soft_info"
                        android:layout_toRightOf="@+id/iv_evething_soft_info"
                        android:text="软件介绍"
                        android:textSize="@dimen/normalTextSize"/>

                    <ImageView
                        android:id="@+id/iv_evething_soft_info_arrow_right"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginEnd="20dp"
                        android:layout_marginRight="20dp"
                        android:src="@mipmap/ic_me_arrow_right_gray"/>
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/line_width"
                    android:background="@color/line_gray"/>

                <RelativeLayout
                    android:id="@+id/rela_feedback"
                    android:layout_width="match_parent"
                    android:background="@drawable/layout_selector_white_system_gray"
                    android:layout_height="50dp">

                    <ImageView
                        android:id="@+id/imageView15"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="15dp"
                        android:layout_marginStart="21dp"
                        android:src="@mipmap/feedback"/>

                    <TextView
                        android:id="@+id/textView72"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"
                        android:layout_marginStart="12dp"
                        android:layout_toEndOf="@+id/imageView15"
                        android:layout_toRightOf="@+id/imageView15"
                        android:text="意见反馈"
                        android:textSize="@dimen/normalTextSize"/>

                    <ImageView
                        android:id="@+id/imageView16"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginEnd="20dp"
                        android:layout_marginRight="20dp"
                        android:src="@mipmap/ic_me_arrow_right_gray"/>

                    <TextView
                        android:textColor="@color/red"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:id="@+id/textView_feedBackCount"
                        android:layout_centerVertical="true"
                        android:layout_toLeftOf="@+id/imageView16"
                        android:layout_toStartOf="@+id/imageView16"
                        android:layout_marginRight="10dp"
                        android:layout_marginEnd="23dp"/>
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/line_width"
                    android:background="@color/line_gray"/>

                <RelativeLayout
                    android:id="@+id/rl_mine_evething_clear_diskcache"
                    android:layout_width="match_parent"
                    android:background="@drawable/layout_selector_white_system_gray"
                    android:layout_height="50dp">

                    <ImageView
                        android:id="@+id/imageView20"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="15dp"
                        android:layout_marginStart="21dp"
                        android:src="@mipmap/aboutsoft"/>

                    <TextView
                        android:id="@+id/textView85"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"
                        android:layout_marginStart="12dp"
                        android:layout_toEndOf="@+id/imageView20"
                        android:layout_toRightOf="@+id/imageView20"
                        android:text="清除缓存"
                        android:textSize="@dimen/normalTextSize"/>

                    <TextView
                        android:id="@+id/tv_diskcache_num"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginEnd="20dp"
                        android:layout_marginRight="20dp"
                        android:src="@mipmap/ic_me_arrow_right_gray"/>
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/line_width"
                    android:background="@color/line_gray"/>

                <RelativeLayout
                    android:layout_marginBottom="20dp"
                    android:id="@+id/relativeLayout26"
                    android:layout_width="match_parent"
                    android:background="@drawable/layout_selector_white_system_gray"
                    android:layout_height="50dp">

                    <ImageView
                        android:id="@+id/imageView34"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="15dp"
                        android:layout_marginStart="21dp"
                        android:src="@mipmap/aboutsoft"/>

                    <TextView
                        android:id="@+id/textView89"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"
                        android:layout_marginStart="12dp"
                        android:layout_toEndOf="@+id/imageView34"
                        android:layout_toRightOf="@+id/imageView34"
                        android:text="检查更新"
                        android:textSize="@dimen/normalTextSize"/>

                    <ImageView
                        android:id="@+id/imageView48"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginEnd="20dp"
                        android:layout_marginRight="20dp"
                        android:src="@mipmap/ic_me_arrow_right_gray"/>
                </RelativeLayout>

            </LinearLayout>

            <!--<Button-->
                <!--android:id="@+id/button_cancle_login"-->
                <!--android:layout_width="match_parent"-->
                <!--android:layout_height="wrap_content"-->
                <!--android:layout_gravity="center_horizontal"-->
                <!--android:layout_marginLeft="80dp"-->
                <!--android:layout_marginRight="80dp"-->
                <!--android:layout_marginTop="20dp"-->
                <!--android:layout_marginBottom="20dp"-->
                <!--android:background="#ff4c20"-->
                <!--android:text="注销账号"-->
                <!--android:textColor="@color/white"-->
                <!--android:textSize="@dimen/normalTextSize"-->
                <!--/>-->
        </LinearLayout>
    </ScrollView>

</LinearLayout>
